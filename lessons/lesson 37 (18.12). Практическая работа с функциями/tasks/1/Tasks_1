def rectangle_info(width, height):
    S = width * height
    Pr = 2*(width + height)
    return (S, Pr)
result = rectangle_info(5, 3)  
print(result)

def is_adult(age):
    if age >=18:
        return True
    else:
        return False

def safe_div(a, b):  
    if b == 0:  
        return None  
    return a / b 
